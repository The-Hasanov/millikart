{
    "name": "the-hasanov/millikart",
    "description": "MilliKart Class for laravel",
    "type": "library",
    "authors": [
        {
            "name": "Hikmet Hesenov",
            "email": "hikmet.hesenov.93@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "illuminate/support": "~5.6 || ~5.7 || ~5.8 || ~6.0 || ~7.0 || ~8.0 || ~9.0",
        "ext-json": "*",
        "ext-simplexml": "*"
    },
    "autoload": {
        "psr-4": {
            "Chameleon\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Chameleon\\MilliKartServiceProvider"
            ],
            "aliases": {
                "MilliKart": "Chameleon\\MilliKartFacade"
            }
        }
    },
    "scripts": {
        "fix": [
            "php-cs-fixer fix src"
        ]
    }
}
